{
  "name": "tango-videos-video-adder",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Add tango videos from youtube chrome extension",
  "icons": {
    "16": "icons/icon-16.png",
    "128": "icons/icon-128.png"
  },
  "web_accessible_resources": [
    "icons/*.png"
  ],
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [
        "styles/tango-videos.css"
      ],
      "js": [
        "scripts/tango-videos.js"
      ]
    }
  ]
}
